                    {
                        "name": "Adress",
                        "in": "path",
                        "description": "attributes related to an user adress",
                        "required": true
                    }
                ],
                "responses": {
                    "405": {
                        "description": "Invalid input"
                    }
                }
            }
        },
        "/login": {
            "get": {
                "tags": [
                    "auth"
                ],
                "summary": "Returns user details and authentication",
                "description": "Returns a map of status codes to quantities",
                "operationId": "login",
                "parameters": [
                    {
                        "name": "user",
