                "operationId": "destroy",
                "parameters": [
                    {
                        "name": "userHashId",
                        "in": "path",
                        "description": "user hash id",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "422": {
                        "description": "Desculpe, o usuario n√£o pode ser excluido."
